Host *
  IgnoreUnknown UseKeychain
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes

Host tp.lan.
  User seven

Host netgear.lan.
  User root
  IdentityFile ~/.ssh/id_rsa.netgear
  KexAlgorithms +diffie-hellman-group14-sha1
  KexAlgorithms +diffie-hellman-group1-sha1

Host mercury.lan.
  User root
  IdentityFile ~/.ssh/id_rsa.netgear

Host openwrt.sevenever.com
  User root
  IdentityFile ~/.ssh/id_rsa.netgear

Host nas.lan.
  User seven
  IdentityFile ~/.ssh/id_rsa.openwrt

Host vps.sevenever.com
  User seven
  Port 28911
  IdentityFile ~/.ssh/id_rsa.netgear

Host rpi.lan.
  User pi
  IdentityFile ~/.ssh/id_rsa.pi

Host rpi-wifi.lan.
  User pi
  IdentityFile ~/.ssh/id_rsa.pi

Host zero.lan.
  User pi
  IdentityFile ~/.ssh/id_rsa.pi

Match host github.com exec "[ -z $ON_WORK_MACHINE ]"
  IdentityFile /home/seven/.ssh/id_rsa.github

Match host github.com exec "[ ! -z $ON_WORK_MACHINE ]"
  IdentityFile ~/.ssh/id_rsa.work

