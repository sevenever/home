Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa.work

Host netgear.lan.
  User root
  IdentityFile ~/.ssh/id_rsa.netgear

